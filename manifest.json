{
  "name": "동영상 배속 재생기",
  "description": "동영상 재생 속도를 설정합니다.",
  "version": "1.4.1",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "128": "res/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "res/icon.png",
    "default_title": "동영상 배속 설정"
  },
  "commands": {
    "set_playback_rate": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "windows": "Ctrl+Period",
        "mac": "Ctrl+ Period"
      },
      "description": "동영상 배속 설정"
    }
  }
}
